  "keywords": [
    "email",
    "validate",
    "e-mail",
    "mail"
  ],
  "author": "hou5emd",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hou5emd/email-validator.git"
  },
  "bugs": {
